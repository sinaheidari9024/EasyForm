                                                    @form.Parts[i].Questions[j].Number .
                                                </label>
                                            </div>
                                            <div class="form-check mx-2">
                                                <input class="form-check-input" type="checkbox" value="" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId-check" onclick="checkForInpuCheckTex(@(JsonConvert.SerializeObject(@form.Parts[i].Questions[j])))">
                                                <label for="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId" class="form-label">
                                                    @form.Parts[i].Questions[j].SpanishText
                                                    @if (form.Parts[i].Questions[j].IsRequierd)
                                                    {
                                                        <small class="font-10">
                                                            <i class="fa fa-asterisk text-danger"></i>
                                                        </small>
                                                    }
                                                </label>
                                            </div>
                                        </div>

                                        <div class="col-12 d-none" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()-box">
                                            <input type="text" class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()-input" />
                                        </div>
                                    </div>
                                }
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.SpecialText)
                            {
                                <div class="mb-3 col-12 d-flex flex-row align-items-center">
                                    <span>@form.Parts[i].Questions[j].Number. </span>
                                    @{
                                        bool isInInputRange = false;
                                        var answerIndex = 0;
                                    }
                                    @for (var k = 0; k < form.Parts[i].Questions[j].SpanishText.Length; k++)
                                    {
                                        @if (form.Parts[i].Questions[j].SpanishText[k].Equals(' '))
                                        {

                                            <span class="special-text-letter-spacing"></span>
                                        }
                                        else if (form.Parts[i].Questions[j].SpanishText[k].Equals('#') && isInInputRange == false)
                                        {
                                            isInInputRange = true;
                                            if (form.Parts[i].Questions[j].Answer.Length > 0)
                                            {
                                                var answerListPush = form.Parts[i].Questions[j].Answer.Split(',');
                                                for (int d = 0; d < answerListPush.Length; d++)
                                                {
                                                    var index = answerListPush[d].IndexOf(':');
                                                    answerListPush[d] = answerListPush[d].Substring(index + 1);
                                                }
                                                <input type="text" class="form-control w-25 mx-1" id="@k.ToString()" value="@answerListPush[answerIndex]" />
                                                answerIndex = answerIndex + 1;
                                            }
                                            else
                                            {
                                                <input type="text" class="form-control w-25 mx-1" id="@k.ToString()" />
                                            }

                                        }
                                        else if (form.Parts[i].Questions[j].SpanishText[k].Equals('#') && isInInputRange == true)
                                        {
                                            isInInputRange = false;
                                        }
                                        else if (isInInputRange == false)
                                        {
                                            <span>@form.Parts[i].Questions[j].SpanishText[k]</span>
                                        }

                                    }
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.CheckBoxTextBox)
                            {
                                <div class="mb-3 col-12 d-flex flex-row align-items-center">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label" style="width: 55px">
                                        @form.Parts[i].Questions[j].Number .
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    @for (var k = 0; k < form.Parts[i].Questions[j].Items.Count; k++)
                                    {
                                        @if (form.Parts[i].Questions[j].Answer != null && form.Parts[i].Questions[j].Answer.Contains(form.Parts[i].Questions[j].Items[k].Id.ToString()))
                                        {
                                            <div class="form-check mx-2">
                                                <input class="form-check-input" checked type="checkbox" value="" id="@form.Parts[i].Questions[j].Items[k].Title@form.Parts[i].Questions[j].Items[k].Id">
                                                <label class="form-check-label" for="@form.Parts[i].Questions[j].Items[k].Title">
                                                    @form.Parts[i].Questions[j].Items[k].SpanishTitle
                                                </label>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="form-check mx-2">
                                                <input class="form-check-input" type="checkbox" value="" id="@form.Parts[i].Questions[j].Items[k].Title@form.Parts[i].Questions[j].Items[k].Id">
                                                <label class="form-check-label" for="@form.Parts[i].Questions[j].Items[k].Title">
                                                    @form.Parts[i].Questions[j].Items[k].SpanishTitle
                                                </label>
                                            </div>
                                        }

                                    }
                                    @if (@form.Parts[i].Questions[j].Answer != null)
                                    {
                                        <input type="text" class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()" value="@form.Parts[i].Questions[j].Answer.Remove(0, form.Parts[i].Questions[j].Answer.IndexOf(":") + 1)" />
                                    }
                                    else
                                    {
                                        <input type="text" class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()" value="" />
                                    }

                                </div>
                            }
                        }
                    </div>
                </div>
            </div>
        }
    </div>
    <div class="w-100 d-flex justify-content-center my-3 px-3">
        <button class="btn btn-primary" id="saveForm" onclick="doFunction(@(JsonConvert.SerializeObject(form)));">
            Update
        </button>
    </div>
</div>



<style>
    .font-10 {
        font-size: 10px;
    }

    .special-text-letter-spacing {
        margin-left: 2px;
        margin-right: 2px;
    }
</style>

<script type="text/javascript">
    $(document).ready(function () {
        spinnerHide();
    });

    function onSelectDropdown(questionTitle, questionId, itemTitle) {
        document.getElementById(questionTitle + questionId).innerHTML = itemTitle;
    }

     function doFunction(form) {
        var answerList = [];
        var userAplicationId = form.UserApplicationId;
        var parts = form.Parts;
        for(var i = 0; i < parts.length; i++) {
            for(var j = 0; j < parts[i].Questions.length; j++) {
                var question = parts[i].Questions[j];
                switch(question.Type) {
                    //TextBox
                    case 0:
                    var questtionElementValue = (document.getElementById(question.Text + question.QuestionId.toString())).value;
                    if(question.IsRequierd == true && !questtionElementValue) {
                        break;
                    } else if (questtionElementValue.length) {
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: questtionElementValue
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }

                    //DropDown
                    case 1:
                    var text = document.getElementById(question.Text + question.QuestionId).innerText;
                    if(text) {
                        const item = question.Items.find(item => item.Title === text);
                        if(question.IsRequierd == true && !item) {
                            break;
                        } else {
                            if(item) {
                            var answer = {
                                QuestionId: question.QuestionId,
                                UserApplicationId: userAplicationId,
                                Text: item.Id.toString()
                            }
                            answerList.push(answer);
                            break;
                            }else {
                                break;
                            }
                    }
                    } else {
                        break;
                    }

                    //Numeric
                    case 2:
                    var questtionElementValue = (document.getElementById(question.Text + question.QuestionId.toString())).value;
                    if(question.IsRequierd == true && !questtionElementValue) {
                        break;
                    } else if (questtionElementValue.length) {
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: questtionElementValue
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }

                    //TextNumeric
                    case 3:
                    var questtionElementValue = (document.getElementById(question.Text + question.QuestionId.toString())).value;
                    if(question.IsRequierd == true && !questtionElementValue) {
                        break;
                    } else if (questtionElementValue.length) {
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: questtionElementValue
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }

                    //Table1
                    case 4:
                    var tableBody = document.getElementById(question.QuestionId + "-table1-body");
                    var rowLength = tableBody.rows.length;
                    var tableDataList = []
                    if(rowLength > 0) {
                        for (i = 0; i < rowLength; i++) {
                         var rowCells = tableBody.rows.item(i).cells;
                         var cellLength = rowCells.length;
                         const rowData = {
                             BenefitReceived: rowCells.item(0).innerHTML,
                             StartDate: rowCells.item(1).innerHTML,
                             EndDate: rowCells.item(2).innerHTML,
                             DollarAmount: rowCells.item(3).innerHTML,
                         }
                         tableDataList.push(rowData);
                        }
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: JSON.stringify(tableDataList)
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }
                    break;

                    //Table2
                    case 5:
                    var tableBody = document.getElementById(question.QuestionId + "-table2-body");
                    var rowLength = tableBody.rows.length;
                    var tableDataList = []
                    if(rowLength > 0) {
                        for (i = 0; i < rowLength; i++) {
                         var rowCells = tableBody.rows.item(i).cells;
                         var cellLength = rowCells.length;
                         const rowData = {
                             InstitutionName: rowCells.item(0).innerHTML,
                             StartDate: rowCells.item(1).innerHTML,
                             EndDate: rowCells.item(2).innerHTML,
                             Reason: rowCells.item(3).innerHTML,
                         }
                         tableDataList.push(rowData);
                        }
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: JSON.stringify(tableDataList)
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }
                    break;
                    //OptionBox
                    case 6:
                    var text = '';
                    for(var k = 0; k < question.Items.length; k++) {
                        var checkbox = document.getElementById(question.Items[k].Title + question.Items[k].Id);
                        if (checkbox.checked) {
                            text = text + question.Items[k].Id + ',';
                        }
                    }
                    if(question.IsRequierd == true && !text.length){
                        break;
                    } else if(text.length) {
                         var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: text.substring(0,text.length - 1)
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }

                    //CheckBox
                    case 7:
                    var text = '';
                    for(var k = 0; k < question.Items.length; k++) {
                        var checkbox = document.getElementById(question.Items[k].Title + question.Items[k].Id);
                        if (checkbox.checked) {
                            text = text + question.Items[k].Id + ',';
                        }
                    }
                    if(question.IsRequierd == true && !text.length){
                        break;
                    } else if(text.length) {
                         var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: text.substring(0,text.length - 1)
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }

                    //Date
                    case 8:
                    var questtionElementValue = (document.getElementById(question.Text + question.QuestionId.toString())).value;
                    if(question.IsRequierd == true && !questtionElementValue) {
                        break;
                    } else if (questtionElementValue.length) {
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: questtionElementValue
                        }
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }

                    //Boolean
                    case 9:
                    var trueElement = document.getElementById(question.Text + question.QuestionId + '-yes').checked;
                    if(trueElement){
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: 'true'
                        };
                        answerList.push(answer);
                        break;
                    } else {
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: 'false'
                        };
                        answerList.push(answer);
                        break;
                    }

                    //TextArea
                    case 11:
                    var textAreaValue = (document.getElementById(question.Text + question.QuestionId.toString())).value;
                    if(textAreaValue != null) {
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: textAreaValue
                        };
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                    }

                    //check
                    case 12:
                    const check = document.getElementById(question.Text + question.QuestionId.toString());
                    if(check.checked == true) {
                        var answer = {
                            QuestionId: question.QuestionId,
                            UserApplicationId: userAplicationId,
                            Text: "checked"
                        };
                        answerList.push(answer);
                        break;
                    } else {
                        break;
                        }

                    //CheckText
                    case 13:
                        const checkElement = document.getElementById(question.Text + question.QuestionId.toString() + '-check');
                        if (checkElement.checked) {
                            const inputElement = document.getElementById(question.Text + question.QuestionId.toString() + '-input');
                            if (inputElement.value) {
                                var answer = {
                                    QuestionId: question.QuestionId,
                                    UserApplicationId: userAplicationId,
                                    Text: "Checked:" + inputElement.value
                                };
                                answerList.push(answer);
                                break;
                            } else {
                                var answer = {
                                    QuestionId: question.QuestionId,
                                    UserApplicationId: userAplicationId,
                                    Text: "Checked:"
                                };
                                answerList.push(answer);
                                break;
                            }
                        } else {
                            break;
                        }

                    //SpecialText
                    case 14:
                        var isInInputRange = false;
                        var textName = '';
                        var inputElement;
                        for (var k = 0; k < question.Text.length; k++) {
                            if (question.Text[k] === "#" && isInInputRange == false) {
                                isInInputRange = true;
                                inputElement = document.getElementById(k.toString());
                            } else if (isInInputRange == true && question.Text[k] !== "#") {
                                textName = textName + question.Text[k]
                            } else if (question.Text[k] === "#" && isInInputRange == true) {
                                isInInputRange = false;
                                textName = textName + ":" + inputElement.value + ','
                            }
                        }
                        if (textName.length > 0) {
                            var answer = {
                                QuestionId: question.QuestionId,
                                UserApplicationId: userAplicationId,
                                Text: textName.substring(0, textName.length - 1)
                            };
                            answerList.push(answer);
                            break;
                        } else {
                            break;
                        }
                    //CheckBoxTextBox
                    case 15:
                        var text = '';
                        for (var k = 0; k < question.Items.length; k++) {
                            var checkbox = document.getElementById(question.Items[k].Title + question.Items[k].Id);
                            if (checkbox.checked) {
                                text = text + question.Items[k].Id + ',';
                            }
                        }
                        text = text.substring(0, text.length - 1);
                        var questtionElementValue = (document.getElementById(question.Text + question.QuestionId.toString())).value;
                        if (questtionElementValue != null) {
                            text = text + ":" + questtionElementValue;
                        }
                        if (text.length > 0) {
                            var answer = {
                                QuestionId: question.QuestionId,
                                UserApplicationId: userAplicationId,
                                Text: text
                            }
                            answerList.push(answer);
                            break;
                        } else {
                            break;
                        }
                    default:
                        break;
                }
            }
        }
        $.ajax({
        type: 'POST',
        url: '@Url.Action("AnswerAsync", "Home")',
        dataType: 'json',
        cache: false,
        contentType: 'application/json',
        data: JSON.stringify(answerList),
            success: function (result) {
                window.location = result.redirectToUrl;
                }
        });
    }

    function checkForInpuCheckTex(question) {
        var checkElement = document.getElementById(question.Text + question.QuestionId + "-check");
        if (checkElement.checked) {
            document.getElementById(question.Text + question.QuestionId + "-box").classList.remove("d-none")
            document.getElementById(question.Text + question.QuestionId + "-box").classList.add("d-block");
        } else {
            document.getElementById(question.Text + question.QuestionId + "-box").classList.remove("d-block");
            document.getElementById(question.Text + question.QuestionId + "-box").classList.add("d-none");
        }
    }

    function onAddTable1(questionId) {
        const benefitReceivedValue = document.getElementById(questionId + "-benefit-received").value;
        const startDateValue = document.getElementById(questionId + "-start-date").value;
        const endDateValue = document.getElementById(questionId + "-end-date").value;
        const dollarAmountValue = document.getElementById(questionId + "-dollar-amount").value;
        const table1Element = document.getElementById(questionId + "-table1-body");
        var table1Row = table1Element.insertRow(0);
        var benefitReceivedCell = table1Row.insertCell(0);
        var startDateCell = table1Row.insertCell(1);
        var endDateCell = table1Row.insertCell(2);
        var dollarAmountCell = table1Row.insertCell(3);
        benefitReceivedCell.innerHTML = benefitReceivedValue;
        startDateCell.innerHTML = startDateValue;
        endDateCell.innerHTML = endDateValue;
        dollarAmountCell.innerHTML = dollarAmountValue;
    }

    function onAddTable2(questionId) {
        const institutionNameValue = document.getElementById(questionId + "-institution-name").value;
        const startDateValue = document.getElementById(questionId + "-start-date").value;
        const endDateValue = document.getElementById(questionId + "-end-date").value;
        const reasonValue = document.getElementById(questionId + "-reason").value;
        const table1Element = document.getElementById(questionId + "-table2-body");
        var table1Row = table1Element.insertRow(0);
        var institutionNameCell = table1Row.insertCell(0);
        var startDateCell = table1Row.insertCell(1);
        var endDateCell = table1Row.insertCell(2);
        var reasonCell = table1Row.insertCell(3);
        institutionNameCell.innerHTML = institutionNameValue;
        startDateCell.innerHTML = startDateValue;
        endDateCell.innerHTML = endDateValue;
        reasonCell.innerHTML = reasonValue;
    }
</script>
