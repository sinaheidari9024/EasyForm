@using EasyForm.Enum
@using EasyForm.ViewModel;
@using Newtonsoft.Json
@model ApplicationPartsVm

@{
    ViewData["Title"] = "Modify Application";
    var form = ViewData.Model;
}

<div>
    <div class="accordion" id="accordionEn">
        @for (int i = 0; i < form.Parts.Count; i++)
        {
            <div class="accordion-item">
                <h2 class="accordion-header" id="flush-headingOne">
                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-@i.ToString()" aria-expanded="false" aria-controls="flush-@i.ToString()">
                        @form.Parts[i].Title

                        @if (form.Parts[i].IsCompleted == true)
                        {
                            <i class="fa fa-check text-success" style="margin: 0 12px"></i>
                        }
                        else
                        {
                            <i class="fa fa-times text-danger" style="margin: 0 12px"></i>
                        }

                    </button>
                </h2>
                <div id="flush-@i.ToString()" class="accordion-collapse collapse pt-4" aria-labelledby="flush-headingOne" data-bs-parent="#accordionFlushExample">
                    <div class="container-fluid row">
                        @for (var j = 0; j < form.Parts[i].Questions.Count; j++)
                        {
                            @if (form.Parts[i].Questions[j].Type == QuestionType.TextBox)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    <input type="text" class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()" value="@form.Parts[i].Questions[j].Answer" />
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.DropDown)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    <div class="dropdown">
                                        <button class="btn btn-outline-secondary dropdown-toggle w-50 text-left d-flex justify-content-between align-items-center" type="button" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId" data-bs-toggle="dropdown" aria-expanded="false">
                                            @if (@form.Parts[i].Questions[j].Answer != null && @form.Parts[i].Questions[j].Answer.Length > 0)
                                            {
                                                <span>@form.Parts[i].Questions[j].Items.Find(item => item.Id == int.Parse(form.Parts[i].Questions[j].Answer))</span>
                                            }
                                            else
                                            {
                                                <span>pick up one item</span>
                                            }
                                        </button>
                                        <ul class="dropdown-menu" aria-labelledby="@form.Parts[i].Questions[j].Text">
                                            @for (var k = 0; k < form.Parts[i].Questions[j].Items.Count; k++)
                                            {
                                                <li onclick="onSelectDropdown(@(JsonConvert.SerializeObject(form.Parts[i].Questions[j].Text)), @(JsonConvert.SerializeObject(form.Parts[i].Questions[j].QuestionId)), @(JsonConvert.SerializeObject(form.Parts[i].Questions[j].Items[k].Title)))"><p class="dropdown-item" id="@form.Parts[i].Questions[j].Items[k].Title@form.Parts[i].Questions[j].Items[k].Id">@form.Parts[i].Questions[j].Items[k].Title</p></li>
                                            }
                                        </ul>
                                    </div>
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.Numeric)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    <input type="number" class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()" value="@form.Parts[i].Questions[j].Answer" />
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.TextNumeric)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    <input type="number" class="form-control" maxlength="@form.Parts[i].Questions[j].Minlengh" maxlength="@form.Parts[i].Questions[j].MaxLengh" length id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()" value="@form.Parts[i].Questions[j].Answer" />
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.Table1)
                            {
                                <div class="mb-3 col-12">
                                    <div>
                                        <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                            @form.Parts[i].Questions[j].Number .
                                            @form.Parts[i].Questions[j].Text
                                            @if (form.Parts[i].Questions[j].IsRequierd)
                                            {
                                                <small class="font-10">
                                                    <i class="fa fa-asterisk text-danger"></i>
                                                </small>
                                            }
                                        </label>
                                    </div>
                                    <div class="d-flex flex-wrap justify-content-between">
                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="benefit-received" class="form-label">Benefit Received</label>
                                            <input type="text" class="form-control" id="@form.Parts[i].Questions[j].QuestionId-benefit-received" placeholder="">
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="start-date" class="form-label">Start Date</label>
                                            <input type="date" placeholder="..." class="form-control" id="@form.Parts[i].Questions[j].QuestionId-start-date" />
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="end-date" class="form-label">End Date</label>
                                            <input type="date" placeholder="..." class="form-control" id="@form.Parts[i].Questions[j].QuestionId-end-date" />
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="dollar-amount" class="form-label">Dollar Amount</label>
                                            <input type="number" placeholder="..." class="form-control" id="@form.Parts[i].Questions[j].QuestionId-dollar-amount" />
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3 d-flex align-items-end">
                                            <button class="btn btn-success" onclick="onAddTable1(@(JsonConvert.SerializeObject(form.Parts[i].Questions[j].QuestionId)))">Add</button>
                                        </div>
                                    </div>

                                    <table class="table w-100 table-striped table-bordered" id="@form.Parts[i].Questions[j].QuestionId-table1">
                                        <thead>
                                            <tr>
                                                <th scope="col">Benefit Received</th>
                                                <th scope="col">Start Date</th>
                                                <th scope="col">End Date</th>
                                                <th scope="col">Dollar Amount</th>
                                            </tr>
                                        </thead>
                                        <tbody id="@form.Parts[i].Questions[j].QuestionId-table1-body">
                                            @if (form.Parts[i].Questions[j].Answer != null && form.Parts[i].Questions[j].Answer.Length > 0)
                                            {
                                                var dataList = JsonConvert.DeserializeObject<List<Table1>>(form.Parts[i].Questions[j].Answer);
                                                for (var k = 0; k < dataList.Count; k++)
                                                {
                                                    <tr>
                                                        <td>@dataList[k].BenefitReceived</td>
                                                        <td>@dataList[k].StartDate</td>
                                                        <td>@dataList[k].EndDate</td>
                                                        <td>@dataList[k].DollarAmount</td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.Table2)
                            {
                                <div class="mb-3 col-12">
                                    <div>
                                        <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                            @form.Parts[i].Questions[j].Number .
                                            @form.Parts[i].Questions[j].Text
                                            @if (form.Parts[i].Questions[j].IsRequierd)
                                            {
                                                <small class="font-10">
                                                    <i class="fa fa-asterisk text-danger"></i>
                                                </small>
                                            }
                                        </label>
                                    </div>
                                    <div class="d-flex flex-wrap justify-content-between">
                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="benefit-received" class="form-label">Institution Name</label>
                                            <input type="text" class="form-control" id="@form.Parts[i].Questions[j].QuestionId-institution-name" placeholder="">
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="start-date" class="form-label">Start Date</label>
                                            <input type="date" placeholder="..." class="form-control" id="@form.Parts[i].Questions[j].QuestionId-start-date" />
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="end-date" class="form-label">End Date</label>
                                            <input type="date" placeholder="..." class="form-control" id="@form.Parts[i].Questions[j].QuestionId-end-date" />
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3">
                                            <label for="dollar-amount" class="form-label">Reason</label>
                                            <input type="text" placeholder="..." class="form-control" id="@form.Parts[i].Questions[j].QuestionId-reason" />
                                        </div>

                                        <div class="mb-3 col-12 col-md-auto p-3 d-flex align-items-end">
                                            <button class="btn btn-success" onclick="onAddTable2(@(JsonConvert.SerializeObject(form.Parts[i].Questions[j].QuestionId)))">Add</button>
                                        </div>
                                    </div>

                                    <table class="table w-100 table-striped table-bordered" id="@form.Parts[i].Questions[j].QuestionId-table2">
                                        <thead>
                                            <tr>
                                                <th scope="col">Institution Name</th>
                                                <th scope="col">Start Date</th>
                                                <th scope="col">End Date</th>
                                                <th scope="col">Reason</th>
                                            </tr>
                                        </thead>
                                        <tbody id="@form.Parts[i].Questions[j].QuestionId-table2-body">
                                            @if (form.Parts[i].Questions[j].Answer != null && form.Parts[i].Questions[j].Answer.Length > 0)
                                            {
                                                var dataList = JsonConvert.DeserializeObject<List<Table2>>(form.Parts[i].Questions[j].Answer);
                                                for (var k = 0; k < dataList.Count; k++)
                                                {
                                                    <tr>
                                                        <td>@dataList[k].InstitutionName</td>
                                                        <td>@dataList[k].StartDate</td>
                                                        <td>@dataList[k].EndDate</td>
                                                        <td>@dataList[k].Reason</td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.OptionBox)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    @for (var k = 0; k < form.Parts[i].Questions[j].Items.Count; k++)
                                    {
                                        @if (form.Parts[i].Questions[j].Items[k].IsChecked == false)
                                        {
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Items[k].Title@form.Parts[i].Questions[j].Items[k].Id">
                                                <label class="form-check-label" for="@form.Parts[i].Questions[j].Items[k].Title">
                                                    @form.Parts[i].Questions[j].Items[k].Title
                                                </label>
                                            </div>
                                        }
                                        @if (form.Parts[i].Questions[j].Items[k].IsChecked == true)
                                        {
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Items[k].Title@form.Parts[i].Questions[j].Items[k].Id" checked>
                                                <label class="form-check-label" for="@form.Parts[i].Questions[j].Items[k].Title">
                                                    @form.Parts[i].Questions[j].Items[k].Title
                                                </label>
                                            </div>
                                        }
                                    }

                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.CheckBox)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    @for (var k = 0; k < form.Parts[i].Questions[j].Items.Count; k++)
                                    {

                                        @if (form.Parts[i].Questions[j].Items[k].IsChecked == false)
                                        {
                                            <div class="form-check">
                                                <input class="form-check-input" type="checkbox" value="" id="@form.Parts[i].Questions[j].Items[k].Title@form.Parts[i].Questions[j].Items[k].Id">
                                                <label class="form-check-label" for="@form.Parts[i].Questions[j].Items[k].Title">
                                                    @form.Parts[i].Questions[j].Items[k].Title
                                                </label>
                                            </div>
                                        }
                                        @if (form.Parts[i].Questions[j].Items[k].IsChecked == true)
                                        {
                                            <div class="form-check">
                                                <input class="form-check-input" type="checkbox" value="" id="@form.Parts[i].Questions[j].Items[k].Title@form.Parts[i].Questions[j].Items[k].Id" checked>
                                                <label class="form-check-label" for="@form.Parts[i].Questions[j].Items[k].Title">
                                                    @form.Parts[i].Questions[j].Items[k].Title
                                                </label>
                                            </div>
                                        }
                                    }
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.Date)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    <input type="date" placeholder="..." class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId" value="@form.Parts[i].Questions[j].Answer" />
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.Boolean)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId-yes">
                                        <label class="form-check-label" for="@form.Parts[i].Questions[j].Text + '-yes'">
                                            Yes
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Text-no">
                                        <label class="form-check-label" for="@form.Parts[i].Questions[j].Text + '-no'">
                                            No
                                        </label>
                                    </div>

                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.Note)
                            {
                                <div class="mb-3 col-12">
                                    <p> @form.Parts[i].Questions[j].Text</p>
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.TextArea)
                            {
                                <div class="mb-3 col-12">
                                    <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                        @form.Parts[i].Questions[j].Number .
                                        @form.Parts[i].Questions[j].Text
                                        @if (form.Parts[i].Questions[j].IsRequierd)
                                        {
                                            <small class="font-10">
                                                <i class="fa fa-asterisk text-danger"></i>
                                            </small>
                                        }
                                    </label>
                                    <textarea class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()" rows="3"></textarea>
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.Check)
                            {
                                <div class="mb-3 col-12 d-flex flex-row align-items-center">
                                    <div>
                                        <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                            @form.Parts[i].Questions[j].Number .
                                        </label>
                                    </div>
                                    <div class="form-check mx-2">
                                        @if (form.Parts[i].Questions[j].Answer != null && form.Parts[i].Questions[j].Answer.Contains("checked"))
                                        {
                                            <input class="form-check-input" type="checkbox" checked value="" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId">
                                        }
                                        else
                                        {
                                            <input class="form-check-input" type="checkbox" value="" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId">
                                        }

                                        <label for="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId" class="form-label">
                                            @form.Parts[i].Questions[j].Text
                                            @if (form.Parts[i].Questions[j].IsRequierd)
                                            {
                                                <small class="font-10">
                                                    <i class="fa fa-asterisk text-danger"></i>
                                                </small>
                                            }
                                        </label>
                                    </div>
                                </div>
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.CheckText)
                            {
                                @if (form.Parts[i].Questions[j].Answer != null)
                                {
                                    <div class="mb-3 col-12 d-flex flex-column align-items-center">
                                        <div class="col-12 d-flex flex-row p-0 mb-3">
                                            <div>
                                                <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                                    @form.Parts[i].Questions[j].Number .
                                                </label>
                                            </div>
                                            <div class="form-check mx-2">
                                                <input class="form-check-input" type="checkbox" checked value="" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId-check" onclick="checkForInpuCheckTex(@(JsonConvert.SerializeObject(@form.Parts[i].Questions[j])))">
                                                <label for="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId" class="form-label">
                                                    @form.Parts[i].Questions[j].Text
                                                    @if (form.Parts[i].Questions[j].IsRequierd)
                                                    {
                                                        <small class="font-10">
                                                            <i class="fa fa-asterisk text-danger"></i>
                                                        </small>
                                                    }
                                                </label>
                                            </div>
                                        </div>

                                        <div class="col-12 d-block" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()-box">
                                            <input type="text" class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()-input" value="@form.Parts[i].Questions[j].Answer.Remove(0, 8)" />
                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div class="mb-3 col-12 d-flex flex-column align-items-center">
                                        <div class="col-12 d-flex flex-row p-0 mb-3">
                                            <div>
                                                <label for="@form.Parts[i].Questions[j].Text" class="form-label">
                                                    @form.Parts[i].Questions[j].Number .
                                                </label>
                                            </div>
                                            <div class="form-check mx-2">
                                                <input class="form-check-input" type="checkbox" value="" name="@form.Parts[i].Questions[j].Text" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId-check" onclick="checkForInpuCheckTex(@(JsonConvert.SerializeObject(@form.Parts[i].Questions[j])))">
                                                <label for="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId" class="form-label">
                                                    @form.Parts[i].Questions[j].Text
                                                    @if (form.Parts[i].Questions[j].IsRequierd)
                                                    {
                                                        <small class="font-10">
                                                            <i class="fa fa-asterisk text-danger"></i>
                                                        </small>
                                                    }
                                                </label>
                                            </div>
                                        </div>

                                        <div class="col-12 d-none" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()-box">
                                            <input type="text" class="form-control" id="@form.Parts[i].Questions[j].Text@form.Parts[i].Questions[j].QuestionId.ToString()-input" />
                                        </div>
                                    </div>
                                }
                            }
                            @if (form.Parts[i].Questions[j].Type == QuestionType.SpecialText)
                            {
                                <div class="mb-3 col-12 d-flex flex-row align-items-center">
                                    <span>@form.Parts[i].Questions[j].Number. </span>
                                    @{
                                        bool isInInputRange = false;
                                        var answerIndex = 0;
                                    }
                                    @for (var k = 0; k < form.Parts[i].Questions[j].Text.Length; k++)
                                    {
                                        @if (form.Parts[i].Questions[j].Text[k].Equals(' '))
                                        {

                                            <span class="special-text-letter-spacing"></span>
                                        }
                                        else if (form.Parts[i].Questions[j].Text[k].Equals('#') && isInInputRange == false)
                                        {
                                            isInInputRange = true;
                                            if (form.Parts[i].Questions[j].Answer != null && form.Parts[i].Questions[j].Answer.Length > 0)
                                            {
                                                var answerListPush = form.Parts[i].Questions[j].Answer.Split(',');
                                                for (int d = 0; d < answerListPush.Length; d++)
                                                {
                                                    var index = answerListPush[d].IndexOf(':');
                                                    answerListPush[d] = answerListPush[d].Substring(index + 1);
                                                }
                                                <input type="text" class="form-control w-25 mx-1" id="@k.ToString()" value="@answerListPush[answerIndex]" />
                                                answerIndex = answerIndex + 1;
                                            }
                                            else
                                            {
                                                <input type="text" class="form-control w-25 mx-1" id="@k.ToString()" />
                                            }

                                        }
